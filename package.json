{
  "name": "makeen-boilerplate",
  "private": "true",
  "engines": {
    "node": "8.x.x"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-node packages/core/src/index.js",
    "test": "babel-node packages/core/src/index.js --inspect",
    "postinstall": "yarn run bootstrap",
    "bootstrap": "lerna bootstrap",
    "clean-modules": "rm -rf node_modules/ && rm -rf packages/*/node_modules && rm -rf packages/*/build",
    "clean-setup": "yarn run clean-modules && yarn",
    "debug": "babel-node --inspect src/index.js",
    "babel-node": "babel-node",
    "lint": "eslint .",
    "watch:files": "makeen-watch",
    "watch:server": "nodemon packages/core/build/index.js",
    "build": "makeen-build",
    "dev": "babel-node packages/core/src/index.js",
    "publish-packages": "npm run build && lerna publish"
  },
  "author": "Victor Zamfir <victor@makeen.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-modern-node": "^3.2.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "core-decorators": "0.19.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-graphql": "0.8.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "husky": "0.13.4",
    "lerna": "^2.0.0",
    "lint-staged": "3.5.1",
    "makeen-cli": "^1.2.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "prettier": "1.5.2"
  },
  "babel": {
    "presets": [
      "modern-node",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-es2015-destructuring",
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "dependencies": {}
}
